    "spec=portfolioSpec() #Markowitz\n",
    "setTargetReturn(spec)=0.015#Randomly chosen doesn't impact end result\n",
    "setRiskFreeRate(spec)=0.00 # Important\n",
    "setNFrontierPoints(spec)=100 # Set for better plot"
   ]
  },
